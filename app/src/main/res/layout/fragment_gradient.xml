<?xml version="1.0" encoding="utf-8"?>
<layout>
<com.chihihx.launcher.view.KeyEventFrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/root"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clipToOutline="false"
    android:clipChildren="false"
    android:clipToPadding="false">
    <View
        android:id="@+id/surface"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

    <com.chihihx.launcher.view.SimpleGradientView
        android:id="@+id/lt"
        android:layout_gravity="left|top"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <com.chihihx.launcher.view.SimpleGradientView
        android:id="@+id/rt"
        android:rotation="90"
        android:layout_gravity="right|top"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <com.chihihx.launcher.view.SimpleGradientView
        android:id="@+id/lb"
        android:rotation="270"
        android:layout_gravity="left|bottom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <com.chihihx.launcher.view.SimpleGradientView
        android:id="@+id/rb"
        android:rotation="180"
        android:layout_gravity="right|bottom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <androidx.appcompat.widget.LinearLayoutCompat
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:text="@string/set_gradient"
            android:textSize="@dimen/qb_px_sp_25"
            android:layout_marginTop="@dimen/qb_px_26"
            android:layout_gravity="center_horizontal"
            android:textColor="@color/text_color_style_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <TextView
            android:layout_gravity="center_horizontal"
            android:text="@string/manual_gradient_tip"
            android:textColor="@color/text_color_style_1"
            android:layout_marginTop="@dimen/qb_px_16"
            android:lineSpacingExtra="5dp"
            android:gravity="center"
            android:maxWidth="@dimen/qb_px_600"
            android:textStyle="bold"
            android:textSize="@dimen/qb_px_sp_16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
    </androidx.appcompat.widget.LinearLayoutCompat>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:clipChildren="false"
        android:clipToPadding="false"
        android:clipToOutline="false"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <FrameLayout
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:id="@+id/content"
            android:clickable="true"
            android:focusable="true"
            android:clipChildren="false"
            android:clipToPadding="false"
            android:clipToOutline="false"
            android:focusableInTouchMode="true"
            android:background="@drawable/menu_item_3"
            android:layout_gravity="center"
            android:orientation="vertical"
            android:layout_width="@dimen/qb_px_400"
            android:layout_height="@dimen/qb_px_225">
            <androidx.cardview.widget.CardView
                app:cardBackgroundColor="@color/transparent"
                app:cardElevation="0dp"
                app:cardCornerRadius="@dimen/qb_px_5"
                android:layout_margin="@dimen/qb_px_6"
                app:cardUseCompatPadding="false"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <ImageView
                    android:src="@drawable/screen_home"
                    android:scaleType="centerCrop"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
                <FrameLayout
                    android:background="@drawable/oval_solid_2"
                    android:layout_gravity="center"
                    android:layout_width="@dimen/qb_px_150"
                    android:layout_height="@dimen/qb_px_150">
                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_gravity="center"
                        android:orientation="vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                        <androidx.appcompat.widget.LinearLayoutCompat
                            android:orientation="horizontal"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">
                            <ImageView
                                android:id="@+id/dir_lr"
                                android:rotation="90"
                                android:src="@drawable/baseline_arrow_left_100"
                                android:layout_width="@dimen/qb_px_50"
                                android:layout_height="@dimen/qb_px_50"/>
                            <TextView
                                android:id="@+id/lr_text"
                                android:textSize="@dimen/qb_px_sp_25"
                                android:textColor="@color/text_color_style_1"
                                android:gravity="center"
                                android:layout_gravity="center_vertical"
                                android:layout_width="@dimen/qb_px_60"
                                android:layout_height="wrap_content"/>
                        </androidx.appcompat.widget.LinearLayoutCompat>
                        <androidx.appcompat.widget.LinearLayoutCompat
                            android:orientation="horizontal"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">
                            <ImageView
                                android:id="@+id/dir_tb"
                                android:rotation="360"
                                android:src="@drawable/baseline_arrow_left_100"
                                android:layout_width="@dimen/qb_px_50"
                                android:layout_height="@dimen/qb_px_50"/>
                            <TextView
                                android:id="@+id/tb_text"
                                android:textColor="@color/text_color_style_1"
                                android:textSize="@dimen/qb_px_sp_25"
                                android:gravity="center"
                                android:layout_gravity="center_vertical"
                                android:layout_width="@dimen/qb_px_60"
                                android:layout_height="wrap_content"/>
                        </androidx.appcompat.widget.LinearLayoutCompat>
                    </androidx.appcompat.widget.LinearLayoutCompat>
                </FrameLayout>
            </androidx.cardview.widget.CardView>
            <View
                android:id="@+id/lt_anchors"
                android:background="@drawable/oval_solid_3"
                android:layout_marginTop="@dimen/qb_px_m_10"
                android:layout_marginLeft="@dimen/qb_px_m_10"
                android:layout_width="@dimen/qb_px_20"
                android:layout_height="@dimen/qb_px_20"/>
            <View
                android:id="@+id/rt_anchors"
                android:layout_gravity="right|top"
                android:background="@drawable/oval_solid_3"
                android:layout_marginTop="@dimen/qb_px_m_10"
                android:layout_marginRight="@dimen/qb_px_m_10"
                android:layout_width="@dimen/qb_px_20"
                android:layout_height="@dimen/qb_px_20"/>
            <View
                android:id="@+id/lb_anchors"
                android:layout_gravity="left|bottom"
                android:background="@drawable/oval_solid_3"
                android:layout_marginBottom="@dimen/qb_px_m_10"
                android:layout_marginLeft="@dimen/qb_px_m_10"
                android:layout_width="@dimen/qb_px_20"
                android:layout_height="@dimen/qb_px_20"/>
            <View
                android:id="@+id/rb_anchors"
                android:layout_gravity="right|bottom"
                android:background="@drawable/oval_solid_3"
                android:layout_marginBottom="@dimen/qb_px_m_10"
                android:layout_marginRight="@dimen/qb_px_m_10"
                android:layout_width="@dimen/qb_px_20"
                android:layout_height="@dimen/qb_px_20"/>
        </FrameLayout>
        <androidx.appcompat.widget.LinearLayoutCompat
            app:layout_constraintTop_toBottomOf="@id/content"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginTop="@dimen/qb_px_46"
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <androidx.appcompat.widget.LinearLayoutCompat
                android:layout_margin="@dimen/qb_px_16"
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <TextView
                    android:text="*"
                    android:textColor="@color/text_color_style_1"
                    android:layout_gravity="center_vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
                <TextView
                    android:layout_gravity="center_vertical"
                    android:text="OK"
                    android:textColor="@color/text_color_style_1"
                    android:textSize="@dimen/qb_px_sp_16"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
                <TextView
                    android:text="*"
                    android:textColor="@color/text_color_style_1"
                    android:layout_gravity="center_vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
                <TextView
                    android:textSize="@dimen/qb_px_sp_16"
                    android:layout_marginLeft="@dimen/qb_px_6"
                    android:textColor="@color/text_color_style_1"
                    android:layout_gravity="center_vertical"
                    android:text="@string/click_ok_changer_dir"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </androidx.appcompat.widget.LinearLayoutCompat>
            <androidx.appcompat.widget.LinearLayoutCompat
                android:layout_margin="@dimen/qb_px_16"
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <TextView
                    android:text="*"
                    android:textSize="@dimen/qb_px_sp_14"
                    android:textColor="@color/text_color_style_1"
                    android:layout_gravity="center_vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
                <ImageView
                    android:layout_gravity="center_vertical"
                    android:src="@drawable/baseline_switch_left_100"
                    android:layout_width="@dimen/qb_px_25"
                    android:layout_height="@dimen/qb_px_25"/>
                <TextView
                    android:text="*"
                    android:textSize="@dimen/qb_px_sp_14"
                    android:textColor="@color/text_color_style_1"
                    android:layout_gravity="center_vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
                <TextView
                    android:textSize="@dimen/qb_px_sp_16"
                    android:layout_marginLeft="@dimen/qb_px_6"
                    android:textColor="@color/text_color_style_1"
                    android:layout_gravity="center_vertical"
                    android:text="@string/vertical_dir"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </androidx.appcompat.widget.LinearLayoutCompat>
            <androidx.appcompat.widget.LinearLayoutCompat
                android:layout_margin="@dimen/qb_px_16"
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <TextView
                    android:text="*"
                    android:textSize="@dimen/qb_px_sp_16"
                    android:textColor="@color/text_color_style_1"
                    android:layout_gravity="center_vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
                <ImageView
                    android:layout_gravity="center_vertical"
                    android:rotation="90"
                    android:src="@drawable/baseline_switch_left_100"
                    android:layout_width="@dimen/qb_px_25"
                    android:layout_height="@dimen/qb_px_25"/>
                <TextView
                    android:text="*"
                    android:textSize="@dimen/qb_px_sp_16"
                    android:textColor="@color/text_color_style_1"
                    android:layout_gravity="center_vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
                <TextView
                    android:textSize="@dimen/qb_px_sp_16"
                    android:layout_marginLeft="@dimen/qb_px_6"
                    android:textColor="@color/text_color_style_1"
                    android:layout_gravity="center_vertical"
                    android:text="@string/horizontal_dir"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </androidx.appcompat.widget.LinearLayoutCompat>
            <androidx.appcompat.widget.LinearLayoutCompat
                android:layout_margin="@dimen/qb_px_16"
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <TextView
                    android:text="*"
                    android:textSize="@dimen/qb_px_sp_14"
                    android:textColor="@color/text_color_style_1"
                    android:layout_gravity="center_vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
                <ImageView
                    android:layout_gravity="center_vertical"
                    android:src="@drawable/baseline_menu_100"
                    android:layout_width="@dimen/qb_px_25"
                    android:layout_height="@dimen/qb_px_25"/>
                <TextView
                    android:text="*"
                    android:textSize="@dimen/qb_px_sp_14"
                    android:textColor="@color/text_color_style_1"
                    android:layout_gravity="center_vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
                <TextView
                    android:textSize="@dimen/qb_px_sp_16"
                    android:layout_marginLeft="@dimen/qb_px_6"
                    android:textColor="@color/text_color_style_1"
                    android:layout_gravity="center_vertical"
                    android:text="@string/click_menu_reset"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </androidx.appcompat.widget.LinearLayoutCompat>
            <TextView
                android:id="@+id/long_click_tip"
                android:textSize="@dimen/qb_px_sp_16"
                android:layout_margin="@dimen/qb_px_16"
                android:textColor="@color/text_color_style_1"
                android:layout_gravity="center_vertical"
                android:text="@string/long_ok_next_action"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </androidx.appcompat.widget.LinearLayoutCompat>
        <ImageView
            android:id="@+id/lt_lr"
            android:rotation="90"
            app:layout_constraintBottom_toTopOf="@id/content"
            app:layout_constraintLeft_toLeftOf="@id/content"
            android:src="@drawable/baseline_navigation_100"
            android:layout_marginLeft="@dimen/qb_px_26"
            android:layout_width="@dimen/qb_px_40"
            android:layout_height="@dimen/qb_px_40"/>
        <ImageView
            android:id="@+id/lt_tb"
            android:rotation="180"
            app:layout_constraintTop_toTopOf="@id/content"
            app:layout_constraintRight_toLeftOf="@id/content"
            android:src="@drawable/baseline_navigation_100"
            android:layout_marginTop="@dimen/qb_px_26"
            android:layout_width="@dimen/qb_px_40"
            android:layout_height="@dimen/qb_px_40"/>

        <ImageView
            android:id="@+id/lb_lr"
            android:rotation="90"
            app:layout_constraintTop_toBottomOf="@id/content"
            app:layout_constraintLeft_toLeftOf="@id/content"
            android:src="@drawable/baseline_navigation_100"
            android:layout_marginLeft="@dimen/qb_px_26"
            android:layout_width="@dimen/qb_px_40"
            android:layout_height="@dimen/qb_px_40"/>
        <ImageView
            android:id="@+id/lb_tb"
            app:layout_constraintBottom_toBottomOf="@id/content"
            app:layout_constraintRight_toLeftOf="@id/content"
            android:src="@drawable/baseline_navigation_100"
            android:layout_marginBottom="@dimen/qb_px_26"
            android:layout_width="@dimen/qb_px_40"
            android:layout_height="@dimen/qb_px_40"/>

        <ImageView
            android:id="@+id/rb_lr"
            android:rotation="270"
            app:layout_constraintTop_toBottomOf="@id/content"
            app:layout_constraintRight_toRightOf="@id/content"
            android:src="@drawable/baseline_navigation_100"
            android:layout_marginRight="@dimen/qb_px_26"
            android:layout_width="@dimen/qb_px_40"
            android:layout_height="@dimen/qb_px_40"/>

        <ImageView
            android:id="@+id/rb_tb"
            app:layout_constraintBottom_toBottomOf="@id/content"
            app:layout_constraintLeft_toRightOf="@id/content"
            android:src="@drawable/baseline_navigation_100"
            android:layout_marginBottom="@dimen/qb_px_26"
            android:layout_width="@dimen/qb_px_40"
            android:layout_height="@dimen/qb_px_40"/>

        <ImageView
            android:id="@+id/rt_lr"
            android:rotation="270"
            app:layout_constraintBottom_toTopOf="@id/content"
            app:layout_constraintRight_toRightOf="@id/content"
            android:src="@drawable/baseline_navigation_100"
            android:layout_marginRight="@dimen/qb_px_26"
            android:layout_width="@dimen/qb_px_40"
            android:layout_height="@dimen/qb_px_40"/>

        <ImageView
            android:id="@+id/rt_tb"
            android:rotation="180"
            app:layout_constraintTop_toTopOf="@id/content"
            app:layout_constraintLeft_toRightOf="@id/content"
            android:src="@drawable/baseline_navigation_100"
            android:layout_marginTop="@dimen/qb_px_26"
            android:layout_width="@dimen/qb_px_40"
            android:layout_height="@dimen/qb_px_40"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</com.chihihx.launcher.view.KeyEventFrameLayout>
</layout>